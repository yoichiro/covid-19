{
  "name": "covid-19",
  "version": "1.0.0",
  "main": "lib/index.js",
  "license": "MIT",
  "engine": {
    "node": "10"
  },
  "scripts": {
    "clean": "rm -rf lib",
    "lint": "eslint 'src/**/*.ts'",
    "build": "yarn lint && yarn clean && tsc",
    "set-project": "gcloud config set project covid-19-6096b",
    "deploy-crawl": "yarn build && yarn set-project && gcloud functions deploy crawl --runtime nodejs10 --trigger-topic crawl",
    "deploy-fulfillment": "yarn build && yarn set-project && gcloud functions deploy fulfillment --runtime nodejs10 --trigger-http",
    "create-index": "yarn set-project && gcloud datastore indexes create ./index.yaml"
  },
  "dependencies": {
    "@google-cloud/datastore": "^5.0.6",
    "actions-on-google": "^2.12.0",
    "firebase-admin": "^8.9.2",
    "firebase-functions": "^3.3.0",
    "jsdom": "^16.2.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.28",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@types/jsdom": "^16.1.0",
    "@types/request": "^2.48.4",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "eslint": "^6.8.0",
    "typescript": "^3.8.3"
  }
}
